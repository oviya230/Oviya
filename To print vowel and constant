

#include <stdio.h>
#include <string.h>

int main() {
    char str[100];
    int i, len;

    // Input string
    printf("Enter a string: ");
    scanf("%s", str);

    len = strlen(str);

    printf("Vowels: ");
    for(i = 0; i < len; i++) {
        if(str[i] == 'a' || str[i] == 'e' || str[i] == 'i' ||
           str[i] == 'o' || str[i] == 'u' ||
           str[i] == 'A' || str[i] == 'E' || str[i] == 'I' ||
           str[i] == 'O' || str[i] == 'U') {
            printf("%c ", str[i]);
        }
    }

    printf("\nConsonants: ");
    for(i = 0; i < len; i++) {
        if(! (str[i] == 'a' || str[i] == 'e' || str[i] == 'i' ||
              str[i] == 'o' || str[i] == 'u' ||
              str[i] == 'A' || str[i] == 'E' || str[i] == 'I' ||
              str[i] == 'O' || str[i] == 'U')) {
            printf("%c ", str[i]);
        }
    }

    printf("\n");
    return 0;
}


