

#include <stdio.h>

int main() {
    int num, i;

    // Input a positive integer
    printf("Enter a positive integer: ");
    scanf("%d", &num);

    printf("Factors of %d are: ", num);

    // Find and print factors
    for(i = 1; i <= num; i++) {
        if(num % i == 0) {
            printf("%d ", i);
        }
    }

    printf("\n");
    return 0;
}
